        42;
    };

    prop2 : AUTO_TYPE <- "String here!";
    method2(e : AUTO_TYPE) : AUTO_TYPE {
        {
            out_string(e);
            e <- "Updated randomly";
            31337;
        }
    };

    (* Completely arbitrary and meaningless randomness *)
    giberish(h : String) : AUTO_TYPE {
        let b: Int <- 0 in {
            while b < 100 loop
                h <- concat(h, "@!#");
                b <- b + 1;
                out_string("Adding nonsense: @!#\n");
            pool;
            h;
        }
    };
};

